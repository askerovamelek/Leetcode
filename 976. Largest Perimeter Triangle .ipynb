   "source": [
    "class Solution:\n",
    "    def largestPerimeter(self, nums: List[int]) -> int:\n",
    "        nums.sort()\n",
    "        for i in range(len(nums)-1, 1, -1):\n",
    "            if nums[i-1] + nums[i-2] > nums[i]:\n",
    "                return nums[i] + nums[i-1] + nums[i-2]\n",
    "        return 0\n"
   ]
  }
