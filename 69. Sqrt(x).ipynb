   "source": [
    "class Solution:\n",
    "    def mySqrt(self, x: int) -> int:\n",
    "        if x < 2:\n",
    "            return x\n",
    "\n",
    "        res = 1\n",
    "        for i in range(1, x//2 + 1):\n",
    "            if i * i <= x:\n",
    "                res = i\n",
    "            else:\n",
    "                break\n",
    "        return res\n"
   ]
  }
